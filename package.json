{
    "name": "wire",
    "version": "0.0.1",
    "description": "Workspace Incident Reporting",
    "main": "index.js",
    "repository": "https://github.com/AndelaOSP/wire.git",
    "author": "Mbarak Mbigo <mbarak.mbigo@andela.com>",
    "license": "MIT",
    "scripts": {
        "babel": "babel",
        "webpack": "webpack",
        "start": "npm run prebuild && npm run copy:html && npm run copy:images && webpack-dev-server --host wire.andela.com  --inline --hot --colors --progress && lint:watch",
        "start:dev": "node server.js",
        "lint": "node_modules/.bin/esw webpack.config.* src tools",
        "lint:watch": "npm run lint -- --watch",
        "copy:html": "cp src/index.html dist/",
        "copy:images": "cp assets/images/* dist/",
        "prebuild": "rm -rf dist && mkdir dist",
        "build": "npm run prebuild && webpack",
        "test": "mocha --require babel-register \"src/**/*.test.jsx\" --require ./test/helpers.jsx --require ./test/dom.jsx --recursive"
    },
    "dependencies": {
        "axios": "^0.17.1",
        "eslint-plugin-import": "^2.8.0",
        "express": "^4.16.2",
        "history": "^4.7.2",
        "html-webpack-plugin": "^2.30.1",
        "js-cookie": "^2.2.0",
        "jwt-decode": "^2.2.0",
        "material-ui": "^0.20.0",
        "path": "^0.12.7",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^4.0.8",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "webpack": "^3.7.1",
        "webpack-dev-middleware": "^1.12.0",
        "webpack-dev-server": "^2.9.2"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.5",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.7",
        "enzyme": "^3.1.0",
        "eslint": "^4.9.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-react": "^7.4.0",
        "eslint-watch": "^3.1.3",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "jsdom": "^11.3.0",
        "mocha": "^4.0.1",
        "node-sass": "^4.5.3",
        "postcss-flexbugs-fixes": "^3.2.0",
        "postcss-inline-rtl": "^0.9.8",
        "postcss-loader": "^2.0.8",
        "react-addons-test-utils": "^15.6.2",
        "react-dev-utils": "^4.1.0",
        "sass-loader": "^6.0.6",
        "sinon": "^4.0.1",
        "style-loader": "^0.19.0",
        "webpack-combine-loaders": "^2.0.3"
    }
}
